{
  "name": "true-video-streaming-agent",
  "version": "1.0.0",
  "description": "Puppeteer-based TRUE video streaming using native screencast() method - WebM/MP4, not screenshots!",
  "main": "video-stream-server.js",
  "scripts": {
    "start": "node patched-rtmp-test.js",
    "dev": "nodemon patched-rtmp-test.js",
    "build": "echo 'Frontend build complete'",
    "deploy": "vercel --prod",
    "deploy:redis": "vercel --prod --env BACKEND_TYPE=redis",
    "deploy:vpc": "vercel --prod --env BACKEND_TYPE=vpc",
    "postinstall": "npx puppeteer browsers install chrome",
    "setup": "node install-openssl.js && cd puppeteer-screen-recorder-main && npm install && npm run build && cd .. && npm run setup:ssl",
    "setup:ssl": "node generate-ssl-auto.js",
    "install:openssl": "node install-openssl.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "puppeteer",
    "video-streaming",
    "screencast",
    "webm",
    "socket.io",
    "real-time"
  ],
  "author": "ScreenCastR Team",
  "license": "MIT",
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "fluent-ffmpeg": "^2.1.3",
    "puppeteer": "^21.11.0",
    "selfsigned": "^3.0.1",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
